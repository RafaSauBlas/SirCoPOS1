    <targets>
      <target name="database" xsi:type="Database" connectionStringName="SirCoLogs" commandType="StoredProcedure" commandText="[dbo].[AddLog]">
        <parameter name="@application" layout="${appsetting:item=name:default=SirCoPOS}" />
        <parameter name="@machineName" layout="${machinename}" />
        <parameter name="@level" layout="${level}" />
        <parameter name="@logger" layout="${logger}" />
        <parameter name="@callsite" layout="${callsite}" />        
        <!--<parameter name="@message" layout="${message}" />-->
        <parameter name="@module" layout="${event-properties:item=module}" />
        <parameter name="@type" layout="${exception:format=Type}" />
        <parameter name="@message" layout="${exception:format=Message}" />
        <parameter name="@exception" layout="${exception:format=toString,Data:maxInnerExceptionLevel=10}" />      
      </target>      
    </targets>    
    <rules>
      <logger name="*" levels="Fatal" writeTo="database" />      
    </rules>
